{
  "name": "poppler-flight",
  "version": "0.1.0",
  "private": true,
  "main": "build/electron.js",
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "cross-env": "^7.0.2",
    "csv-stringify": "^5.5.1",
    "express": "^4.17.1",
    "json2csv": "^5.0.4",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "node-poppler": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.3",
    "react-query": "^2.26.2",
    "react-scripts": "4.0.0",
    "react-table": "^7.6.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "node server",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "yarn build-dev && yarn electron",
    "electron-web-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "postinstall": "build install-app-deps",
    "electron-pack:mac": "build --mac -c.extraMetadata.main=build/electron.js",
    "electron-pack:win": "build --win -c.extraMetadata.main=build/electron.js",
    "electron-pack:linux": "build --linux -c.extraMetadata.main=build/electron.js",
    "publish:mac": "cross-env GH_TOKEN=30ad63d94b2c0a87fb51ae49320cbb1f7e1bdd68 build --mac --publish always -c.extraMetadata.main=build/electron.js",
    "publish:win": "cross-env GH_TOKEN=30ad63d94b2c0a87fb51ae49320cbb1f7e1bdd68 build --win --publish always -c.extraMetadata.main=build/electron.js",
    "publish:linux": "cross-env GH_TOKEN=30ad63d94b2c0a87fb51ae49320cbb1f7e1bdd68 build --linux --publish always -c.extraMetadata.main=build/electron.js",
    "make-cert": "build create-self-signed-cert -p cert",
    "pack": "yarn electron-pack:mac && yarn electron-pack:linux",
    "dist": "yarn publish:mac && yarn publish:win && yarn publish:linux"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "3.0.0",
    "electron-builder": "20.34.0",
    "electron-log": "^4.3.0"
  }
}
